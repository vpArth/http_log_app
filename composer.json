{
    "name": "arth/http_log_app",
    "type": "project",
    "license": "proprietary",
    "description": "Demo of arth/http_log_bundle (Test Task)",
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "path",
            "url": "../http_log_bundle"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "arth/http_log_bundle": "*",
        "composer/package-versions-deprecated": "1.x-dev",
        "doctrine/doctrine-bundle": "2.3.x-dev",
        "doctrine/doctrine-migrations-bundle": "3.1.x-dev",
        "doctrine/orm": "3.0.x-dev",
        "symfony/console": "^5.0",
        "symfony/dotenv": "^5.0",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "^5.0",
        "symfony/proxy-manager-bridge": "5.2.*",
        "symfony/twig-bundle": "5.2.*",
        "symfony/yaml": "^5.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.2.*"
        }
    }
}
